{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:path:default",
    "fs:allow-write-text-file",
    "fs:allow-read-text-file",
    "fs:allow-appdata-write",
    "fs:allow-appdata-read",
    "fs:scope-appdata",
    "dialog:default"
  ],
  "platform": {
    "darwin": {
      "windows": ["main"]
    },
    "linux": {
      "windows": ["main"]
    },
    "windows": {
      "windows": ["main"]
    }
  }
}